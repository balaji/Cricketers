                            <ListBox x:Name="teams" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Name}" Style="{StaticResource PhoneTextLargeStyle}" Foreground="{StaticResource PhoneInverseInactiveBrush}" TextWrapping="Wrap" />
